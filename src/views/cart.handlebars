<h1>{{title}}</h1>
<div class="cart-container">
    {{#each products}}
        <div class="product">
            <img src="{{this.product.thumbnail}}" alt="{{this.product.title}}">
            <h2>{{this.product.title}}</h2>
            <p>{{this.product.description}}</p>
            <p>Price: ${{this.product.price}}</p>
            <p>Quantity: {{this.quantity}}</p>
        </div>
    {{/each}}
</div>